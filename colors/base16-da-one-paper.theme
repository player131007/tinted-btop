# name: Da One Paper
# author: NNB (https://github.com/NNBnh)
# slug: da-one-paper
# slug-underscored: da_one_paper
# system: base16
# variant: light

# Main bg
theme[main_bg]="#faf0dc"

# Main text color
theme[main_fg]="#181818"

# Title color for boxes
theme[title]="#181818"

# Highlight color for keyboard shortcuts
theme[hi_fg]="#5890f8"

# Background color of selected item in processes box
theme[selected_bg]="#c8c8c8"

# Foreground color of selected item in processes box
theme[selected_fg]="#181818"

# Color of inactive/disabled text
theme[inactive_fg]="#282828"

# Misc colors for processes box including mini cpu graphs, details memory graph and details status text
theme[proc_misc]="#5890f8"

# Cpu box outline color
theme[cpu_box]="#76a85d"

# Memory/disks box outline color
theme[mem_box]="#ff9470"

# Net up/down box outline color
theme[net_box]="#c173d1"

# Processes box outline color
theme[proc_box]="#64b5a7"

# Box divider line and small boxes line color
theme[div_line]="#282828"

# Temperature graph colors
theme[temp_start]="#76a85d"
theme[temp_mid]="#b3684f"
theme[temp_end]="#de5d6e"

# CPU graph colors
theme[cpu_start]="#76a85d"
theme[cpu_mid]="#b3684f"
theme[cpu_end]="#de5d6e"

# Mem/Disk free meter
theme[free_start]="#76a85d"

# Mem/Disk cached meter
theme[cached_start]="#b3684f"

# Mem/Disk available meter
theme[available_start]="#ff9470"

# Mem/Disk used meter
theme[used_start]="#de5d6e"

# Download graph colors
theme[download_start]="#c173d1"
theme[download_mid]="#5890f8"
theme[download_end]="#64b5a7"

# Upload graph colors
theme[upload_start]="#c173d1"
theme[upload_mid]="#5890f8"
theme[upload_end]="#64b5a7"
