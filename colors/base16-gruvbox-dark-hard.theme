# name: Gruvbox dark, hard
# author: Dawid Kurek (dawikur@gmail.com), morhetz (https://github.com/morhetz/gruvbox)
# slug: gruvbox-dark-hard
# slug-underscored: gruvbox_dark_hard
# system: base16
# variant: dark

# Main bg
theme[main_bg]="#1d2021"

# Main text color
theme[main_fg]="#d5c4a1"

# Title color for boxes
theme[title]="#d5c4a1"

# Highlight color for keyboard shortcuts
theme[hi_fg]="#83a598"

# Background color of selected item in processes box
theme[selected_bg]="#3c3836"

# Foreground color of selected item in processes box
theme[selected_fg]="#d5c4a1"

# Color of inactive/disabled text
theme[inactive_fg]="#bdae93"

# Misc colors for processes box including mini cpu graphs, details memory graph and details status text
theme[proc_misc]="#83a598"

# Cpu box outline color
theme[cpu_box]="#b8bb26"

# Memory/disks box outline color
theme[mem_box]="#fe8019"

# Net up/down box outline color
theme[net_box]="#d3869b"

# Processes box outline color
theme[proc_box]="#8ec07c"

# Box divider line and small boxes line color
theme[div_line]="#bdae93"

# Temperature graph colors
theme[temp_start]="#b8bb26"
theme[temp_mid]="#fabd2f"
theme[temp_end]="#fb4934"

# CPU graph colors
theme[cpu_start]="#b8bb26"
theme[cpu_mid]="#fabd2f"
theme[cpu_end]="#fb4934"

# Mem/Disk free meter
theme[free_start]="#b8bb26"

# Mem/Disk cached meter
theme[cached_start]="#fabd2f"

# Mem/Disk available meter
theme[available_start]="#fe8019"

# Mem/Disk used meter
theme[used_start]="#fb4934"

# Download graph colors
theme[download_start]="#d3869b"
theme[download_mid]="#83a598"
theme[download_end]="#8ec07c"

# Upload graph colors
theme[upload_start]="#d3869b"
theme[upload_mid]="#83a598"
theme[upload_end]="#8ec07c"
