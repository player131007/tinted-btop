# name: Gruvbox dark, pale
# author: Dawid Kurek (dawikur@gmail.com), morhetz (https://github.com/morhetz/gruvbox)
# slug: gruvbox-dark-pale
# slug-underscored: gruvbox_dark_pale
# system: base16
# variant: dark

# Main bg
theme[main_bg]="#262626"

# Main text color
theme[main_fg]="#dab997"

# Title color for boxes
theme[title]="#dab997"

# Highlight color for keyboard shortcuts
theme[hi_fg]="#83adad"

# Background color of selected item in processes box
theme[selected_bg]="#3a3a3a"

# Foreground color of selected item in processes box
theme[selected_fg]="#dab997"

# Color of inactive/disabled text
theme[inactive_fg]="#949494"

# Misc colors for processes box including mini cpu graphs, details memory graph and details status text
theme[proc_misc]="#83adad"

# Cpu box outline color
theme[cpu_box]="#afaf00"

# Memory/disks box outline color
theme[mem_box]="#ff8700"

# Net up/down box outline color
theme[net_box]="#d485ad"

# Processes box outline color
theme[proc_box]="#85ad85"

# Box divider line and small boxes line color
theme[div_line]="#949494"

# Temperature graph colors
theme[temp_start]="#afaf00"
theme[temp_mid]="#ffaf00"
theme[temp_end]="#d75f5f"

# CPU graph colors
theme[cpu_start]="#afaf00"
theme[cpu_mid]="#ffaf00"
theme[cpu_end]="#d75f5f"

# Mem/Disk free meter
theme[free_start]="#afaf00"

# Mem/Disk cached meter
theme[cached_start]="#ffaf00"

# Mem/Disk available meter
theme[available_start]="#ff8700"

# Mem/Disk used meter
theme[used_start]="#d75f5f"

# Download graph colors
theme[download_start]="#afaf00"
theme[download_mid]="#ffaf00"
theme[download_end]="#d75f5f"

# Upload graph colors
theme[upload_start]="#afaf00"
theme[upload_mid]="#ffaf00"
theme[upload_end]="#d75f5f"
