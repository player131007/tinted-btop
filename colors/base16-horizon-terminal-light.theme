# Main bg
theme[main_bg]="#fdf0ed"

# Main text color
theme[main_fg]="#403c3d"

# Title color for boxes
theme[title]="#403c3d"

# Highlight color for keyboard shortcuts
theme[hi_fg]="#26bbd9"

# Background color of selected item in processes box
theme[selected_bg]="#fadad1"

# Foreground color of selected item in processes box
theme[selected_fg]="#403c3d"

# Color of inactive/disabled text
theme[inactive_fg]="#948c8a"

# Misc colors for processes box including mini cpu graphs, details memory graph and details status text
theme[proc_misc]="#26bbd9"

# Cpu box outline color
theme[cpu_box]="#29d398"

# Memory/disks box outline color
theme[mem_box]="#f9cec3"

# Net up/down box outline color
theme[net_box]="#ee64ac"

# Processes box outline color
theme[proc_box]="#59e1e3"

# Box divider line and small boxes line color
theme[div_line]="#948c8a"

# Temperature graph colors
theme[temp_start]="#29d398"
theme[temp_mid]="#fadad1"
theme[temp_end]="#e95678"

# CPU graph colors
theme[cpu_start]="#29d398"
theme[cpu_mid]="#fadad1"
theme[cpu_end]="#e95678"

# Mem/Disk free meter
theme[free_start]="#29d398"

# Mem/Disk cached meter
theme[cached_start]="#fadad1"

# Mem/Disk available meter
theme[available_start]="#f9cec3"

# Mem/Disk used meter
theme[used_start]="#e95678"

# Download graph colors
theme[download_start]="#ee64ac"
theme[download_mid]="#26bbd9"
theme[download_end]="#59e1e3"

# Upload graph colors
theme[upload_start]="#ee64ac"
