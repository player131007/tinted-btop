# Main bg
theme[main_bg]="#191724"

# Main text color
theme[main_fg]="#e0def4"

# Title color for boxes
theme[title]="#e0def4"

# Highlight color for keyboard shortcuts
theme[hi_fg]="#c4a7e7"

# Background color of selected item in processes box
theme[selected_bg]="#1f1d2e"

# Foreground color of selected item in processes box
theme[selected_fg]="#e0def4"

# Color of inactive/disabled text
theme[inactive_fg]="#908caa"

# Misc colors for processes box including mini cpu graphs, details memory graph and details status text
theme[proc_misc]="#c4a7e7"

# Cpu box outline color
theme[cpu_box]="#31748f"

# Memory/disks box outline color
theme[mem_box]="#f6c177"

# Net up/down box outline color
theme[net_box]="#f6c177"

# Processes box outline color
theme[proc_box]="#9ccfd8"

# Box divider line and small boxes line color
theme[div_line]="#908caa"

# Temperature graph colors
theme[temp_start]="#31748f"
theme[temp_mid]="#ebbcba"
theme[temp_end]="#eb6f92"

# CPU graph colors
theme[cpu_start]="#31748f"
theme[cpu_mid]="#ebbcba"
theme[cpu_end]="#eb6f92"

# Mem/Disk free meter
theme[free_start]="#31748f"

# Mem/Disk cached meter
theme[cached_start]="#ebbcba"

# Mem/Disk available meter
theme[available_start]="#f6c177"

# Mem/Disk used meter
theme[used_start]="#eb6f92"

# Download graph colors
theme[download_start]="#f6c177"
theme[download_mid]="#c4a7e7"
theme[download_end]="#9ccfd8"

# Upload graph colors
theme[upload_start]="#f6c177"
theme[upload_mid]="#c4a7e7"
theme[upload_end]="#9ccfd8"
