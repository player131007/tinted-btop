# Main bg
theme[main_bg]="#140607"

# Main text color
theme[main_fg]="#5d484e"

# Title color for boxes
theme[title]="#5d484e"

# Highlight color for keyboard shortcuts
theme[hi_fg]="#8b87af"

# Background color of selected item in processes box
theme[selected_bg]="#2a1a1c"

# Foreground color of selected item in processes box
theme[selected_fg]="#5d484e"

# Color of inactive/disabled text
theme[inactive_fg]="#523e43"

# Misc colors for processes box including mini cpu graphs, details memory graph and details status text
theme[proc_misc]="#8b87af"

# Cpu box outline color
theme[cpu_box]="#579523"

# Memory/disks box outline color
theme[mem_box]="#aa301b"

# Net up/down box outline color
theme[net_box]="#682e50"

# Processes box outline color
theme[proc_box]="#e8a766"

# Box divider line and small boxes line color
theme[div_line]="#523e43"

# Temperature graph colors
theme[temp_start]="#579523"
theme[temp_mid]="#cfc9ff"
theme[temp_end]="#90002a"

# CPU graph colors
theme[cpu_start]="#579523"
theme[cpu_mid]="#cfc9ff"
theme[cpu_end]="#90002a"

# Mem/Disk free meter
theme[free_start]="#579523"

# Mem/Disk cached meter
theme[cached_start]="#cfc9ff"

# Mem/Disk available meter
theme[available_start]="#aa301b"

# Mem/Disk used meter
theme[used_start]="#90002a"

# Download graph colors
