# variant: dark

# Main bg
theme[main_bg]="#001e26"

# Main text color
theme[main_fg]="#c0c0b0"

# Title color for boxes
theme[title]="#c0c0b0"

# Highlight color for keyboard shortcuts
theme[hi_fg]="#2075c7"

# Background color of selected item in processes box
theme[selected_bg]="#002731"

# Foreground color of selected item in processes box
theme[selected_fg]="#c0c0b0"

# Color of inactive/disabled text
theme[inactive_fg]="#979e96"

# Misc colors for processes box including mini cpu graphs, details memory graph and details status text
theme[proc_misc]="#2075c7"

# Cpu box outline color
theme[cpu_box]="#728905"

# Memory/disks box outline color
theme[mem_box]="#a57705"

# Net up/down box outline color
theme[net_box]="#c61b6e"

# Processes box outline color
theme[proc_box]="#259185"

# Box divider line and small boxes line color
theme[div_line]="#979e96"

# Temperature graph colors
theme[temp_start]="#728905"
theme[temp_mid]="#708183"
theme[temp_end]="#d01b24"

# CPU graph colors
theme[cpu_start]="#728905"
theme[cpu_mid]="#708183"
theme[cpu_end]="#d01b24"

# Mem/Disk free meter
theme[free_start]="#728905"

# Mem/Disk cached meter
theme[cached_start]="#708183"

# Mem/Disk available meter
theme[available_start]="#a57705"

# Mem/Disk used meter
theme[used_start]="#d01b24"

# Download graph colors
theme[download_start]="#728905"
theme[download_mid]="#708183"
theme[download_end]="#d01b24"

# Upload graph colors
theme[upload_start]="#728905"
theme[upload_mid]="#708183"
theme[upload_end]="#d01b24"
