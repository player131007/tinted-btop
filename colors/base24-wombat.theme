# name: Wombat
# author: FredHappyface (https://github.com/fredHappyface)
# slug: wombat
# slug-underscored: wombat
# system: base24
# variant: dark

# Main bg
theme[main_bg]="#171717"

# Main text color
theme[main_fg]="#b2afa6"

# Title color for boxes
theme[title]="#b2afa6"

# Highlight color for keyboard shortcuts
theme[hi_fg]="#5da9f6"

# Background color of selected item in processes box
theme[selected_bg]="#000000"

# Foreground color of selected item in processes box
theme[selected_fg]="#b2afa6"

# Color of inactive/disabled text
theme[inactive_fg]="#87857f"

# Misc colors for processes box including mini cpu graphs, details memory graph and details status text
theme[proc_misc]="#5da9f6"

# Cpu box outline color
theme[cpu_box]="#b1e869"

# Memory/disks box outline color
theme[mem_box]="#ead89c"

# Net up/down box outline color
theme[net_box]="#e86aff"

# Processes box outline color
theme[proc_box]="#82fff6"

# Box divider line and small boxes line color
theme[div_line]="#87857f"

# Temperature graph colors
theme[temp_start]="#b1e869"
theme[temp_mid]="#a5c7ff"
theme[temp_end]="#ff605a"

# CPU graph colors
theme[cpu_start]="#b1e869"
theme[cpu_mid]="#a5c7ff"
theme[cpu_end]="#ff605a"

# Mem/Disk free meter
theme[free_start]="#b1e869"

# Mem/Disk cached meter
theme[cached_start]="#a5c7ff"

# Mem/Disk available meter
theme[available_start]="#ead89c"

# Mem/Disk used meter
theme[used_start]="#ff605a"

# Download graph colors
theme[download_start]="#e86aff"
theme[download_mid]="#5da9f6"
theme[download_end]="#82fff6"

# Upload graph colors
theme[upload_start]="#e86aff"
theme[upload_mid]="#5da9f6"
theme[upload_end]="#82fff6"
